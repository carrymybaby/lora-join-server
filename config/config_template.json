{
  "database": {
    "mysql": {
      "username": "username",
      "password": "password",
      "database": "mysql",
      "host": "localhost",
      "port": 3306,
      "dialect": "mysql",
      "operatorsAliases": false,
      "logging": false,
      "timezone": "+08:00",
      "define": {
        "freezeTableName": true,
        "timestamp": true,
        "charset": "utf8"
      }
    }
  },
  "mocha": {
    "timeout": 5000,
    "longTimeOut": 15000
  },
  "log": {
    "level": "debug",
    "colorize": true
  },
  "mqClient_js": {
    "consumerGroup": {
      "options": {
        "kafkaHost": "localhost:9092",
        "groupId": "lora-join-server-consumer",
        "sessionTimeout": 15000,
        "protocol": [
          "roundrobin"
        ],
        "fromOffset": "latest"
      },
      "topics": [
        "JS-sub"
      ]
    },
    "client": {
      "kafkaHost": "localhost:9092",
      "clientId": "lora-join-server-produce"
    },
    "producer": {
      "requireAcks": 1,
      "ackTimeoutMs": 100,
      "partitionerType": 2,
      "joinServerTopic": "JS-pub"
    }
  }
}
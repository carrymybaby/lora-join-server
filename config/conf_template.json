{
  "database": {
    "mysql": {
      "username": "username",
      "password": "password",
      "database": "mysql",
      "host": "localhost",
      "port": 3306,
      "dialect": "mysql",
      "operatorsAliases": false,
      "logging": false,
      "timezone": "+08:00",
      "define": {
        "freezeTableName": true,
        "timestamp": true,
        "charset": "utf8"
      }
    }
  },
  "mocha": {
    "timeout": 5000
  },
  "log": {
    "level": "debug",
    "colorize": true
  },
  "mqClient_js": {
    "consumerGroup": {
      "options": {
        "kafkaHost": "kafkaHost:port",
        "groupId": "lora-join-server-consumer",
        "sessionTimeout": 15000,
        "protocol": ["roundrobin"],
        "fromOffset": "latest"
      },
      "topics": [
        "topicA"
      ]
    },
    "client": {
      "kafkaHost": "kafkaHost:port",
      "clientId": "lora-join-server-producer"
    },
    "producer": {
      "requireAcks": 1,
      "ackTimeoutMs": 100,
      "partitionerType": 2,
      "joinServerTopic": "topicB"
    }
  }
}
